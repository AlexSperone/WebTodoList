<div class="col-md-12">
    @if (Items == null)
    {
        <div class="alert alert-info">
            <p>Nothing to do :)</p>
        </div>
    }
    else
    {
        <ul class="list-unstyled">
            @foreach (var item in Items)
            {
                <li>
                    <span>@item.Text</span>
                </li>
            }
        </ul>
    }
</div>

@code {
    [Parameter]
    public IEnumerable<ListItemViewModel> Items { get; set; }
}
